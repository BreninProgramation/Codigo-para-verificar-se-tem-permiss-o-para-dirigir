int main()
{
    int idade;

    printf ("\nDigite sua idade:");
    scanf ("%d",&idade);

    if (idade>=18)
        {
        printf ("\nPermissao para dirigir");
    }
    else {
            printf ("\nPermissao recusada para dirigir");
        }
    return 0;
}
